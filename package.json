{
  "name": "trillo",
  "version": "1.0.0",
  "description": "Trillo booking app flights hotels tours...",
  "main": "index.js",
  "browserslist": "last 10 versions",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass sass/main.scss:css/style.css",
    "prefix:css": "postcss css/style.css --use autoprefixer  -o css/style.prefix.css",
    "compress:css": "sass --style=compressed css/style.prefix.css css/style.css",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Jonas",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "concat": "^1.0.3",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "sass": "^1.66.1",
    "postcss": "^8.4.30"
  }
}
